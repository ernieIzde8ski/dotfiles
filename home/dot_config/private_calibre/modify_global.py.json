#!/usr/bin/env python3
# get real
# vim: filetype=python
# pyright: reportAny=false, reportUnusedCallResult=false

import json
import sys
from typing import TypeAlias # python 3.11 support

JsonObject: TypeAlias = str | float | None | list["JsonObject"] | dict[str, "JsonObject"]
data: str = sys.stdin.read()
if not data:
    print("{}")
    exit()
obj: JsonObject = json.loads(data)
assert isinstance(obj, dict)

obj["case_sensitive"]   = False
obj["filename_pattern"] = "(?P<author>[^_]+) - (?P<title>.+)"
obj["language"]         = "en"
obj["mark_new_books"]   = False
obj.setdefault("output_format", "epub")

res = json.dumps(obj, indent=2, sort_keys=True)
print(res, end="")
